class Solution {
    public int finalValueAfterOperations(String[] operations) {
        int x=0;
        for(String s: operations)
        {
            char ch1=s.charAt(0);
            char ch2=s.charAt(2);
            if(ch1=='+' || ch2=='+')
                x++;
            else
                x--;
        }
        return x;
    }
}
